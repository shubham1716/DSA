



            ////  largest element in array


class Solution {
  public:
    int largest(vector<int> &arr) {
        // code here
        int n=arr.size();
        int larg=arr[0];
        for(int i=0;i<n;i++){
            if(arr[i]>larg){
            larg=arr[i];
                
            }
             
        }
         return larg;
      
    }
};
